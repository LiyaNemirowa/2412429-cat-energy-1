  "description": "Личный проект «Кэт энерджи» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.4.0",
    "browser-sync": "2.27.7",
    "cssnano": "7.0.3",
    "del": "7.1.0",
    "gulp": "4.0.2",
    "gulp-html-bemlinter": "4.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "3.0.0",
    "gulp-imagemin": "9.1.0",
    "gulp-include": "2.4.1",
    "gulp-less": "5.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "10.0.0",
    "gulp-stacksvg": "4.0.0",
    "gulp-terser": "2.1.0",
    "gulp-w3c-html-validator": "5.3.0",
    "gulp-webp": "5.0.0",
    "imagemin-mozjpeg": "10.0.0",
    "imagemin-pngquant": "10.0.0",
    "imagemin-svgo": "11.0.1",
    "postcss-sort-media-queries": "5.2.0",
    "sirv-cli": "2.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.11",
    "stylelint-order-htmlacademy": "2.1.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "lint": "gulp lintHtml && run editorconfig && npm run stylelint",
    "build": "gulp build",
    "preview": "sirv build",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not < 0.25%"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "type": "module"
}
